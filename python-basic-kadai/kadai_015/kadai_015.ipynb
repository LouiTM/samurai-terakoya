        "class Human:\n",
        "  def __init__ (self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def print_name(self):\n",
        "    print(f\"私の名前は{self.name}です。\")\n",
        "\n",
        "  def print_age(self):\n",
        "    print(f\"私の年は{self.age}才です。\")\n",
        "\n",
        "person1 = Human(\"田中\",20)\n",
        "person1.print_name()\n",
        "person1.print_age()\n"
